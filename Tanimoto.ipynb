    "#Tanimoto\n",
    "import cv2\n",
    "import numpy as np\n",
    "import pickle\n",
    "import glob\n",
    "import os\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "import matplotlib.pyplot as plt\n",
    "from rdkit import Chem\n",
    "from rdkit.DataStructs import FingerprintSimilarity\n",
    "from itertools import permutations\n",
    "\n",
    "path_txt = \"D:/ML/IR_test/fig/resize/txt/*.txt\"\n",
    "text_data = []\n",
    "inchi_data = []\n",
    "mol_data = []\n",
    "mass_data = []\n",
    "mol_num = []\n",
    "formula = []\n",
    "labels = []  # Initialize the list to store labels\n",
    "\n",
    "for file in glob.glob(path_txt):\n",
    "    with open(file, 'r') as f:\n",
    "        lines = f.readlines()\n",
    "        if len(lines) >= 6:\n",
    "            sdbs = lines[0].strip()  #SDBS No.\n",
    "            text = lines[1].strip()  # IUPAC name\n",
    "            inchi = lines[2].strip() #InChI\n",
    "            mol = lines[3].strip()   # Molecular formula\n",
    "            mass = float(lines[4].strip())  # Molecular weight\n",
    "            cas = lines[5].strip()  #CAS No.\n",
    "\n",
    "            # Check the format of InChI and add "InChI=" if missing\n",
    "            if not inchi.startswith(\"InChI=\"):\n",
    "                inchi = \"InChI=\" + inchi\n",
    "\n",
    "            text_data.append(text)\n",
    "            inchi_data.append(inchi)\n",
    "            mol_data.append(mol)\n",
    "            mass_data.append(mass)\n",
    "            \n",
    "            # Extract the label from the filename and add it to the list\n",
    "            label = os.path.basename(file).split(\".\")[0]\n",
    "            labels.append(label)\n",
    "\n",
    "# Number of compounds\n",
    "inchi_list = inchi_data  # List of InChI\n",
    "\n",
    "num_compounds = len(inchi_list)\n",
    "\n",
    "# Generate a list of RDKit molecule objects from InChI\n",
    "molecules = []\n",
    "for i, inchi in enumerate(inchi_list):\n",
    "    try:\n",
    "        mol = Chem.MolFromInchi(inchi)\n",
    "        if mol is not None:\n",
    "            molecules.append(mol)\n",
    "        else:\n",
    "            print(f\"Error in processing InChI: {inchi} (file: {glob.glob(path_txt)[i]})\")\n",
    "    except Exception as e:\n",
    "        print(f\"Error in processing InChI: {inchi} (file: {glob.glob(path_txt)[i]})\")\n",
    "        print(f\"Error message: {str(e)}\")\n",
    "\n",
    "# Generate molecular fingerprints\n",
    "fingerprints = [Chem.RDKFingerprint(mol) for mol in molecules]\n",
    "\n",
    "# Initialize a matrix to store Tanimoto coefficients\n",
    "similarity_matrix = [[0.0] * num_compounds for _ in range(num_compounds)]\n",
    "\n",
    "# Calculate Tanimoto coefficients for all pairs of compounds\n",
    "for i in range(num_compounds):\n",
    "    for j in range(i+1, num_compounds):\n",
    "        similarity = FingerprintSimilarity(fingerprints[i], fingerprints[j])\n",
    "        similarity_matrix[i][j] = similarity\n",
    "        similarity_matrix[j][i] = similarity\n",
    "\n",
    "# Output the results\n",
    "for i in range(num_compounds):\n",
    "    for j in range(num_compounds):\n",
    "        label_i = os.path.splitext(os.path.basename(glob.glob(path_txt)[i]))[0]\n",
    "        label_j = os.path.splitext(os.path.basename(glob.glob(path_txt)[j]))[0]\n",
    "        #print(f\"Tanimoto coefficient({label_i}, {label_j}):\", similarity_matrix[i][j])\n",
    "\n",
    "# Specify the list of compounds you want to compare\n",
    "target_compounds = [\"XXXX\", \"XXXX\", \"XXXX\", \"XXXX\", \"XXXX\"]\n",
    "\n",
    "# Calculate the Tanimoto coefficient for all combinations within the specified compounds\n",
    "for i in range(len(target_compounds)):\n",
    "    for j in range(i + 1, len(target_compounds)):\n",
    "        label_i = target_compounds[i]\n",
    "        label_j = target_compounds[j]\n",
    "\n",
    "        # Find the indices of compounds in the specified pair\n",
    "        index_i = labels.index(label_i)\n",
    "        index_j = labels.index(label_j)\n",
    "\n",
    "        # Calculate Tanimoto coefficient for the pair\n",
    "        similarity = FingerprintSimilarity(fingerprints[index_i], fingerprints[index_j])\n",
    "\n",
    "        print(f\"Tanimoto coefficient({label_i}, {label_j}):\", similarity)"
   ]
